<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FlashLight1.MainPage">

    <StackLayout>
        <Button x:Name="turnOn" Text="turn on" Clicked="turnOn_Clicked" HorizontalOptions="CenterAndExpand"/>
        <Button x:Name="turnOff" Text="turn off" Clicked="turnOff_Clicked" HorizontalOptions="CenterAndExpand"/>
    </StackLayout>

</ContentPage>
